{
  "name": "houston-api",
  "version": "1.0.0",
  "description": "GraphQL API for the Astronomer Platform",
  "scripts": {
    "build": "prisma generate && babel src -d dist --copy-files",
    "start": "prisma generate && yarn migrate && nodemon --exec babel-node -e js,graphql src/index.js",
    "serve": "yarn migrate && node dist/index.js",
    "worker": "node dist/workers/index.js",
    "worker:dev": "babel-node src/workers/index.js",
    "test": "prisma generate && jest",
    "coverage": "prisma generate && jest --coverage",
    "lint": "prisma generate && eslint src --ignore-pattern src/lib/generated --ext .js --ext .graphql",
    "token": "prisma token",
    "playground": "graphql playground",
    "migrate": "yarn knex-migrate && yarn prisma-deploy",
    "knex": "babel-node node_modules/.bin/knex",
    "knex-migrate": "yarn knex migrate:latest",
    "prisma-deploy": "prisma deploy --force",
    "cleanup-deployments": "prisma generate && babel-node src/scripts/cleanup-deployments/index.js",
    "upgrade-deployments": "prisma generate && babel-node src/scripts/upgrade-deployments/index.js",
    "expire-deployments": "prisma generate && babel-node src/scripts/expire-deployments/index.js",
    "check-platfrom-updates": "prisma generate && babel-node src/scripts/check-platform-updates/index.js"
  },
  "author": "Astronomer",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "analytics-node": "^3.4.0-beta.1",
    "apollo-server": "^2.14.3",
    "apollo-server-express": "^2.14.3",
    "bcryptjs": "^2.4.3",
    "commander": "github:astronomer/commander",
    "config": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "elasticsearch": "^15.4.1",
    "email-templates": "^7.0.5",
    "express": "^4.16.4",
    "express-prom-bundle": "^5.1.5",
    "generate-password": "^1.5.0",
    "global-agent": "1.13.1",
    "got": "^9.6.0",
    "graphql": "^14.0.2",
    "graphql-binding": "^2.5.2",
    "graphql-import": "^0.7.1",
    "graphql-playground-html": "^1.6.25",
    "graphql-playground-middleware-express": "^1.7.18",
    "graphql-tools": "^4.0.3",
    "graphql-type-json": "^0.2.1",
    "grpc": "^1.23.2",
    "grpc-caller": "^0.13.0",
    "haikunator": "^2.0.0",
    "handlebars": "^4.7.6",
    "handlebars-layouts": "^3.1.4",
    "http-status-codes": "^1.3.0",
    "https-proxy-agent": ">=2.2.3",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.21.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "ms": "^2.1.1",
    "nats": "^1.4.9",
    "node-forge": "^0.9.1",
    "node-nats-streaming": "^0.3.2",
    "openid-client": "^3.15.3",
    "pg": "^7.18.2",
    "pg-connection-string": "^2.0.0",
    "prettier": "^1.19.1",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.10",
    "prisma-client-lib": "^1.34.10",
    "prom-client": "^11.5.3",
    "semver": "^6.3.0",
    "shortid": "^2.2.14",
    "stripe": "^6.36.0",
    "winston": "^3.2.0",
    "yamljs": "^0.3.0",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "graphql-cli": "^4.0.1-beta.0",
    "jest": "^24.8.0",
    "nock": "^11.6.0",
    "nodemon": "^1.18.7",
    "supertest": "^4.0.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
